<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Console App Demo (Common, Android)</_PropertySheetDisplayName>
  </PropertyGroup>

  <PropertyGroup>
    <ocean_consoleapp_common_imported>True</ocean_consoleapp_common_imported>
  </PropertyGroup>

  <!-- Pre-set OceanBuildType for Debug/Release configurations (no Static/Shared prefix) -->
  <PropertyGroup Condition="'$(OceanBuildType)' == ''">
    <OceanBuildType>static</OceanBuildType>
  </PropertyGroup>

  <ImportGroup Label="Dependencies">
    <Import Condition="'$(ocean_applicationdemobase_build_imported)' != 'True'" Project="..\..\application_demo_base_build.props"/>
    <Import Condition="'$(ocean_java_app_build_imported)' != 'True'" Project="..\..\..\..\ocean_java_app_build.props"/>
    <Import Condition="'$(ocean_basejnijava_use_imported)' != 'True'" Project="..\..\..\..\..\..\ocean\base\jni\basejnijava_use.props"/>
    <Import Condition="'$(ocean_platformandroidapplicationjnijava_use_imported)' != 'True'" Project="..\..\..\..\..\..\ocean\platform\android\application\jni\platformandroidapplicationjnijava_use.props"/>
    <Import Condition="'$(ocean_consolenative_use_imported)' != 'True'" Project="..\consolenative\consolenative_use.props"/>
  </ImportGroup>
</Project>
